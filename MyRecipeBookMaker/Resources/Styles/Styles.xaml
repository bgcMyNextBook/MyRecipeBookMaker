    <Style ApplyToDerivedTypes="True" TargetType="Page">
        <Setter Property="Padding" Value="0" />
        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
    </Style>

    <Style ApplyToDerivedTypes="True" TargetType="Shell">
        <Setter Property="Shell.BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="Shell.ForegroundColor" Value="{dx:ThemeColor OnSurfaceVariant}" />
        <Setter Property="Shell.TitleColor" Value="{dx:ThemeColor OnSurface}" />
        <Setter Property="Shell.NavBarHasShadow" Value="False" />
        <Setter Property="Shell.TabBarBackgroundColor" Value="{dx:ThemeColor SurfaceContainer}" />
        <Setter Property="Shell.TabBarForegroundColor" Value="{dx:ThemeColor Primary}" />
        <Setter Property="Shell.TabBarTitleColor" Value="{dx:ThemeColor Primary}" />
        <Setter Property="Shell.TabBarUnselectedColor" Value="{dx:ThemeColor OnSurfaceVariant}" />
    </Style>

    <Style TargetType="NavigationPage">
        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="BarTextColor" Value="{AppThemeBinding Light={StaticResource Gray200}, Dark={StaticResource White}}" />
        <Setter Property="IconColor" Value="{AppThemeBinding Light={StaticResource Gray200}, Dark={StaticResource White}}" />
    </Style>

    <Style TargetType="TabbedPage">
        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="BarTextColor" Value="{AppThemeBinding Light={StaticResource Magenta}, Dark={StaticResource White}}" />
        <Setter Property="UnselectedTabColor" Value="{AppThemeBinding Light={StaticResource Gray200}, Dark={StaticResource Gray950}}" />
        <Setter Property="SelectedTabColor" Value="{AppThemeBinding Light={StaticResource Gray950}, Dark={StaticResource Gray200}}" />
    </Style>
    <Style TargetType="inputLayout:SfTextInputLayout">
        <Setter Property="EnableHintAnimation" Value="True" />
        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="ContainerType" Value="Outlined" />
        <Setter Property="ContainerBackground" Value="{dx:ThemeColor Surface}" />

        <Setter Property="EnableHintAnimation" Value="True" />
    </Style>
    <Style TargetType="editors:SfComboBox">

        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="Stroke" Value="{dx:ThemeColor Outline}" />
        <Setter Property="DropDownBackground" Value="{dx:ThemeColor Primary}" />
        <Setter Property="DropDownItemTextColor" Value="{dx:ThemeColor OnPrimary}" />
        <Setter Property="DropDownIconColor" Value="{dx:ThemeColor Secondary}" />
        <Setter Property="DropDownStroke" Value="{dx:ThemeColor Outline}" />
        <Setter Property="SelectionTextHighlightColor" Value="{dx:ThemeColor Secondary}" />
        <Setter Property="SelectedDropDownItemBackground" Value="{dx:ThemeColor Secondary}" />
        <Setter Property="ClearButtonIconColor" Value="{dx:ThemeColor Secondary}" />
        <Setter Property="PlaceholderColor" Value="{dx:ThemeColor Outline}" />

    </Style>
    <Style TargetType="radialMenu:SfRadialMenu">

        <Setter Property="BackgroundColor" Value="{dx:ThemeColor Surface}" />
        <Setter Property="CenterButtonBackgroundColor" Value="{dx:ThemeColor Primary}" />
        <Setter Property="CenterButtonStroke" Value="{dx:ThemeColor OnPrimary}" />
        <Setter Property="CenterButtonTextColor" Value="{dx:ThemeColor OnPrimary}" />
        <Setter Property="RimColor" Value="{dx:ThemeColor Secondary}" />
        <Setter Property="SeparatorColor" Value="{dx:ThemeColor Outline}" />
        <Setter Property="CenterButtonBackTextColor" Value="{dx:ThemeColor Secondary}" />
        

    </Style>

</ResourceDictionary>
